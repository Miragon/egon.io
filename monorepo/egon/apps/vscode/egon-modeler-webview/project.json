{
  "name": "egon-modeler-webview",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/vscode/egon-modeler-webview/src",
  "tags": [],
  "targets": {
    "observe": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "development",
      "options": {
        "watch": true,
        "outputPath": "dist/apps/vscode/egon-io/webview",
        "configFile": "apps/vscode/egon-modeler-webview/vite.config.mts",
        "emptyOutDir": false
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": false,
          "vendorChunk": true
        }
      }
    },
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/vscode/egon-io/webview",
        "configFile": "apps/vscode/egon-modeler-webview/vite.config.mts",
        "tsConfig": "apps/vscode/egon-modeler-webview/tsconfig.app.json"
      }
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "egon-modeler-webview:build"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/vscode/egon-modeler-webview/jest.config.ts"
      }
    }
  }
}
