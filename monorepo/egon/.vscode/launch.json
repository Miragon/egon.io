{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run egon.io",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--disable-extensions",
        "--extensionDevelopmentPath=${workspaceFolder}/dist/apps/vscode/egon-io"
      ],
      "outFiles": [
        "${workspaceFolder}/dist/apps/vscode/egon-io/**/*.js"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/dist/apps/vscode/egon-io/**",
        "!**/node_modules/**"
      ],
      "sourceMapPathOverrides": {
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*",
        "webpack://?:*/*": "${workspaceFolder}/apps/vscode/egon-modeler-plugin/*"
      }
    }
  ]
}
