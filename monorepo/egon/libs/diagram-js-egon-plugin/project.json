        "tsConfig": "libs/diagram-js-egon-plugin/tsconfig.lib.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/diagram-js-egon-plugin/jest.config.ts"
      }
    }
  }
}
